def bubblesort(v[]: int; n: int) {
    var i=0, j: int;
    var trocou = 1: int;
    if(i < n -1 && trocou == 1){
        trocou = 0;
        for (j=0; j< n -i - 1; j+=1) {
             if (v[j] > v[j+1]) {
                var aux:int;
                aux = v[j];
                v[j] = v[j+1];
                //v[j+1] = aux;
                trocou = 1;
             }
        }
    }
    write "fim";
}


