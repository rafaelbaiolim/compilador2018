v[4] = 1;
v[5] = 0;

def bubblesort(x[]: int; n: int) {
    var i=0, j: int;
    var trocou = 1: int;
    while (i < n-1 && trocou) {
        trocou = 0;
        for (j=0; j<(n-i-1); j+=1) {
            if (x[j] > x[j+1]) {
                var aux = x[j]: int;
                x[j] = x[j+1];
                x[j+1] = aux;
                trocou = 1;
            }
        }
        i += 1;
    }
}


